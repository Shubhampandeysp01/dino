{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug ts Program",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "program": "${workspaceFolder}/web/components/dashboard/dashboard-feature.tsx",
        "runtimeArgs": ["-r", "ts-node/register"],
        "console": "integratedTerminal",
        "sourceMaps": true,
        "outFiles": [
          "${workspaceFolder}/**/*.js"
        ]
      }
    ]
  }
  